{
  "name": "uswift-dashboard",
  "version": "0.1.0",
  "private": true,
  "description": "Uswift dashboard - Chrome extension for job application automation",
  "author": "Uswift Team",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "chrome-extension",
    "job-automation",
    "dashboard"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:webpack": "next dev",
    "dev:fast": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next dev",
    "dev:debug": "next dev --inspect",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:dev": "npm run dev",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next",
     "clean:install": "rimraf node_modules package-lock.json && npm install", 
    "fix:lock": "rimraf package-lock.json node_modules && npm install"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@studio-freight/lenis": "^1.0.42",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.56.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.5.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
